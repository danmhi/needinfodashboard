{
  "bugzillaconfig": {
    "bugzilla_search_url": "https://{domain}/rest/bug?",
    "bugzilla_put_url": "https://{domain}/rest/bug/{id}",
    "bugzilla_link_url": "https://{domain}/show_bug.cgi?id={id}",
    "bugzilla_user_url": "https://{domain}/rest/user?match={value}",
    "bugzilla_domain": "bugzilla.mozilla.org",
    "bugzilla_test_domain": "bugzilla-dev.allizom.org",
    "use_test_domain": false,
    "fields_query": "f1=requestees.login_name&o1=equals&v1={id}&v2=needinfo%3F&o2=equals&f2=flagtypes.name&exclude_fields=_all",
    "bugs_query": "f1=requestees.login_name&o1=equals&v1={id}&v2=needinfo%3F&o2=equals&f2=flagtypes.name&include_fields=id,summary,comments,flags,severity,priority,assigned_to,op_sys"
  }
}
